package t_tok03;

// 실제 비즈니스 로직을 처리하는 클래스
public class ItemTarget {
	public void selectItem() {
		System.out.println("아이템을 조회합니다.");
	}
	
	// 아이템 구매
	public void buyItem() {
		System.out.println("아이템을 구매합니다.");
	}
	
	public void afterAdvice() {
		System.out.println("$$$$$$$$$ 아이템을 구매해주셔서 감사합니다.$$$$$$$$$$$");
	}
}
